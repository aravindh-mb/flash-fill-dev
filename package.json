{
  "name": "@aravindh-arumugam/flash-fill",
  "version": "1.0.0-beta.6",
  "description": "A developer-only form autofill helper with single-click full form fill.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify --external react",
    "build:browser": "tsup src/browser.ts --format iife --globalName FlashFill --outDir dist --minify --no-dts",
    "lint": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindh-mb/flash-fill-dev.git"
  },
  "keywords": [
    "autofill",
    "form",
    "developer-tool",
    "faker",
    "testing"
  ],
  "author": "Aravindh Arumugam",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^9.5.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4",
    "vue": "^3.5.29"
  }
}